{
    "device": "cuda:0",
    "MAX_EPOCHES": 60,

    "dataset": {
      "dataset_name": "Phoenix",
      "batch_size": 4,
      "resize_shape": [512, 288],
      "seg_classes": 64
    },

    "optim": {
      "lr": 15e-3,
      "momentum": 0.9,
      "weight_decay": 1e-4,
      "nesterov": true
    },

    "lr_scheduler": {
      "warmup": 20,
      "max_iter": 1500,
      "min_lrs": 1e-10
    },

    "model": {
      "scale_exist": 0.07
    }
  }